  "bugs": {
    "url": "https://github.com/N-liraj-khanna/Art-Arena/issues"
  },
  "homepage": "https://github.com/N-liraj-khanna/Art-Arena#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
